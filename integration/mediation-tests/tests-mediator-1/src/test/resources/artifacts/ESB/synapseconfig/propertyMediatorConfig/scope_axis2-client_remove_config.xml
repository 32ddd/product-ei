    <proxy name="integerVal_axis2-client_scope_remove" transports="http,https">
        <target>
            <inSequence>
                <switch xmlns:m0="http://services.samples" source="//m0:getQuote/m0:request/m0:symbol">
                    <default>
                        <property xmlns="http://ws.apache.org/ns/synapse" name="IntVal" value="123"  scope="axis2-client" type="INTEGER" />
                    </default>
                </switch>
                <log level="custom">
                    <property xmlns:ns="http://org.apache.synapse/xsd" xmlns:ns3="http://org.apache.synapse/xsd" name="symbol" expression="$axis2:IntVal" />
                </log>

                <switch xmlns:m0="http://services.samples" source="//m0:getQuote/m0:request/m0:symbol">
                    <default>
                        <property xmlns="http://ws.apache.org/ns/synapse" name="IntVal" action="remove"  scope="axis2-client"/>
                    </default>
                </switch>
                <log level="custom">
                    <property xmlns:ns="http://org.apache.synapse/xsd" xmlns:ns3="http://org.apache.synapse/xsd" name="symbol" expression="$axis2:IntVal" />
                </log>

                <send>
                    <endpoint>
                        <address uri="http://localhost:9000/services/SimpleStockQuoteService" />
                    </endpoint>
                </send>
            </inSequence>
            <outSequence>
                <send/>
            </outSequence>
        </target>
    </proxy>

