<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://ws.apache.org/ns/synapse">

    <proxy name="doubleVal_transport_scope" transports="http,https">
        <target>
            <inSequence>
                <switch xmlns:m0="http://services.samples" source="//m0:getQuote/m0:request/m0:symbol">
                    <default>
                        <property xmlns="http://ws.apache.org/ns/synapse" name="DoubleVal" value="123123.123123"  scope="transport" type="DOUBLE" />
                    </default>
                </switch>
                <log level="custom">
                    <property xmlns:ns="http://org.apache.synapse/xsd" xmlns:ns3="http://org.apache.synapse/xsd" name="symbol" expression="$trp:DoubleVal" />
                </log>
                <send>
                    <endpoint>
                        <address uri="http://localhost:9000/services/SimpleStockQuoteService" />
                    </endpoint>
                </send>
            </inSequence>
            <outSequence>
                <send/>
            </outSequence>
        </target>
    </proxy>

